<h1>Listing backend_owners</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Country</th>
    <th>City</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @backend_owners.each do |backend_owner| %>
  <tr>
    <td><%= backend_owner.name %></td>
    <td><%= backend_owner.country %></td>
    <td><%= backend_owner.city %></td>
    <td><%= link_to 'Show', backend_owner_path(backend_owner) %></td>
    <td><%= link_to 'Edit', edit_backend_owner_path(backend_owner) %></td>
    <td><%= link_to 'Destroy', backend_owner_path(backend_owner), confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Owner', new_backend_owner_path %>
