<% @cars.each do |car| %>
  <tr>
    <td><%= car.model %></td>
    <td><%= car.year %></td>
    <td><%= car.specs %></td>
    <td><%= car.engin_size %></td>
    <td><%= car.gearbox %></td>
    <td><%= car.shifts %></td>
    <td><%= car.type %></td>
    <td><%= car.cylinders %></td>
    <td><%= car.valves %></td>
    <td><%= car.injection %></td>
    <td><%= car.torque %></td>
    <td><%= car.hourse_power %></td>
    <td><%= car.drive_wheels %></td>
    <td><%= car.seats %></td>
    <td><%= car.doors %></td>
    <td><%= car.fuel_consumption %></td>
    <td><%= car.fuel_tank %></td>
    <td><%= car.air_bags %></td>
    <td><%= car.air_bags_front %></td>
    <td><%= car.electric_seat %></td>
    <td><%= car.abs %></td>
    <td><%= car.ebd %></td>
    <td><%= car.esp %></td>
    <td><%= car.alarm %></td>
    <td><%= car.tier_size %></td>
    <td><%= car.front_fog_light %></td>
    <td><%= car.rear_fog_light %></td>
    <td><%= car.electric_mirrors %></td>
    <td><%= car.electric_windows %></td>
    <td><%= link_to 'Show', car %></td>
    <td><%= link_to 'Edit', edit_car_path(car) %></td>
    <td><%= link_to 'Destroy', car, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

