<h1>Listing cars</h1>

<table>
  <tr>
    <th>Year</th>
    <th>Cylinders</th>
    <th>Horse power</th>
    <th>Doors</th>
    <th>Air bags</th>
    <th>Abs</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cars.each do |car| %>
  <tr>
    <td><%= car.year %></td>
    <td><%= car.cylinders %></td>
    <td><%= car.horse_power %></td>
    <td><%= car.doors %></td>
    <td><%= car.air_bags %></td>
    <td><%= car.abs %></td>
    <td><%= link_to 'Show', car %></td>
    <td><%= link_to 'Edit', edit_car_path(car) %></td>
    <td><%= link_to 'Destroy', car, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Car', new_car_path %>
