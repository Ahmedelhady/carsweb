    <th>Drive wheels</th>
    <th>Seats</th>
    <th>Doors</th>
    <th>Fuel consumption</th>
    <th>Fuel tank</th>
    <th>Air bags</th>
    <th>Air bags front</th>
    <th>Electric seat</th>
    <th>Abs</th>
    <th>Ebd</th>
